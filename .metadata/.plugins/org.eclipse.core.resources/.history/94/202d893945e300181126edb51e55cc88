package Common;

public class Customer {

	private String name;

	private String reg;

	private String ride;

	private String time;

	private String slot;
	
	Customer(String n,String r,String t,String s){
		slot = s;
		setName(n);
		setReg(r);
		time = t;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public String getReg() {
		return reg;
	}

	public void setReg(String reg) {
		this.reg = reg;
	}
	
}
